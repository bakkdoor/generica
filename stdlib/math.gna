(define <=
  (lambda (x y)
    (if (= x y)
        t
      (< x y))))

(define >=
  (lambda (x y)
    (if (= x y)
        t
      (> x y))))

(define even?
  (lambda (x)
    (= (% x 2) 0)))

(define odd?
  (lambda (x)
    (not (even? x))))

(define sum (list)
  (reduce 0
          (lambda (x y)
              (+ x y))
          list))

(define product (list)
  (reduce 1 
          (lambda (x y)
            (* x y))
          list))